<script type="text/javascript">
/* jquery unminified for Size Converter page starts here */ /* Coded by Codehouse - Aaliyan Gul */
$(document).ready(function() {
    $(document).on('click', '.header-tabs a', function(e) {
        $('.header-tabs a').removeClass('active');
        $('.section-tabs').hide();
        $(this).addClass('active');
        var activtab = $(this).attr('href');
        $(activtab).show();
        return !1
    });
    $('#size-in').on('change', function() {
        var valsizein = $(this).find(':selected').data('size');
        $('.size-in-inch, .size-in-cntm').hide();
        $('.'+valsizein).show();
    });
    $(document).on('click', '#newsletter-link, #open-newsletter', function(e) {
        $('#section-newsletter').show();
        $('html, body').css({overflow: 'hidden',height: '100%'})
    });
    $(document).on('click', '.newsletter-close', function(e) {
        $('#section-newsletter').hide();
        $('html, body').css({overflow: 'auto',height: 'auto'})
    });
    // find size in lab size
    $('#band-size').on('change', function() {
        var valcupsize = $(this).find(':selected').val();
        var cupsize = $('#cup-size');
        $('#cup-size').val('0');
        $('#resultsize').text('FIND NOW');
        $('option', cupsize).filter(function() {
            if ($(this).attr('data-group') === valcupsize || $(this).attr('data-group') === 'Show') {
                if ($(this).parent('span').length) { $(this).unwrap() }
            } else {
                if (!$(this).parent('span').length) { $(this).wrap("<span>").parent().hide() }
            }
        })
    });
    $('#band-size').trigger('change');
    $('#cup-size').on('change', function() {
        $('#resultsize').text($('#cup-size').val());
        $('#resultcontent').html('the letter might not be what<br>You are used to, but trust us it works!');
    });
    // find size in inches
    $('#band-size-inch').on('change', function() {
        var valbustsizeinch = $(this).find(':selected').val();
        var bustsizeinch = $('#bust-size-inch');
        $('#bust-size-inch').val('0');
        $('#m-resultsize').text('FIND NOW');
        $('#m-result-regular-size').text('SELECT');
        $('option', bustsizeinch).filter(function() {
            if ($(this).attr('data-group') === valbustsizeinch || $(this).attr('data-group') === 'Show') {
                if ($(this).parent('span').length) { $(this).unwrap() }
            } else {
                if (!$(this).parent('span').length) { $(this).wrap("<span>").parent().hide() }
            }
        })
    });
    $('#band-size-inch').trigger('change');
    $('#bust-size-inch').on('change', function() {
        $('#m-resultsize').text($('#bust-size-inch').val());
        $('#m-resultcontent').html('the letter might not be what<br>You are used to, but trust us it works!');
        $('#m-result-regular-size').text($('#bust-size-inch').find(':selected').data('regular'));
        $('#m-result-regular-content').html('this is your traditional bra size');
    });
    // find size in centi meter
    $('#band-size-cntm').on('change', function() {
        var valbustsizecntm = $(this).find(':selected').val();
        var bustsizecntm = $('#bust-size-cntm');
        $('#bust-size-cntm').val('0');
        $('#m-resultsize').text('FIND NOW');
        $('#m-result-regular-size').text('SELECT');
        $('option', bustsizecntm).filter(function() {
            if ($(this).attr('data-group') === valbustsizecntm || $(this).attr('data-group') === 'Show') {
                if ($(this).parent('span').length) { $(this).unwrap() }
            } else {
                if (!$(this).parent('span').length) { $(this).wrap("<span>").parent().hide() }
            }
        })
    });
    $('#band-size-cntm').trigger('change');
    $('#bust-size-cntm').on('change', function() {
        $('#m-resultsize').text($('#bust-size-cntm').val());
        $('#m-resultcontent').html('the letter might not be what<br>You are used to, but trust us it works!');
        $('#m-result-regular-size').text($('#bust-size-cntm').find(':selected').data('regular'));
        $('#m-result-regular-content').html('this is your traditional bra size');
    });
});
</script>
